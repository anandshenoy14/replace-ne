{
    "name": "replace-ne",
    "displayName": "replace-ne",
    "description": "Simple Utility to remove notEmpty(object) checks while transforming Marko v2 components to Marko v3",
    "version": "0.0.1",
    "publisher": "anandshenoy14",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.replaceNotEmpty"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.replaceNotEmpty",
            "title": "Click here to Replace NotEmpty in a template.marko file"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}